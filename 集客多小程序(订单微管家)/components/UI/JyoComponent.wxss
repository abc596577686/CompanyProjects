.Jyo-DateTime {
  width: 750rpx;
  height: 816rpx;
  font-family: 'PingFang', 'Hiragino Sans GB', 'Microsoft YaHei', Arial;
}
.Jyo-DateTime .Jyo-DateTimeTopBar {
  position: relative;
  width: 100%;
  height: 156rpx;
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeLeftArrow {
  position: absolute;
  left: 0;
  top: 0;
  min-width: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  padding-left: 19rpx;
  padding-right: 50rpx;
  font-weight: bold;
  font-size: 70rpx;
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeLeftArrow::after {
  content: "<";
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeRightArrow {
  position: absolute;
  right: 0;
  top: 0;
  min-width: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  padding-left: 50rpx;
  padding-right: 19rpx;
  font-weight: bold;
  font-size: 70rpx;
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeRightArrow::after {
  content: ">";
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeSelectedDateTitle {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-weight: bold;
  font-size: 24px;
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeWeekFrame {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
}
.Jyo-DateTime .Jyo-DateTimeTopBar .Jyo-DateTimeWeekFrame view {
  float: left;
  width: calc(100% / 7);
  text-align: center;
  font-size: 32rpx;
}
.Jyo-DateTime .Jyo-DateTimeDateFrame {
  width: 100%;
  height: 660rpx;
}
.Jyo-DateTime .Jyo-DateTimeDateFrame .Jyo-DateTimeDay {
  position: relative;
  float: left;
  width: calc(100% / 7);
  text-align: center;
  height: 110rpx;
  line-height: 110rpx;
}
.Jyo-DateTime .Jyo-DateTimeDateFrame .Jyo-DateTimeDay view {
  position: absolute;
  left: 15%;
  top: 15%;
