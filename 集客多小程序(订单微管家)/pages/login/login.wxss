    flex-direction: row;
    align-items: center;
    justify-content: space-between;
  }
  
  .goodsNumberWrap .label, .taxWrap .label {
    font-size: 28rpx;
  }
  
  .goodsNumberWrap .handle {
    /* width: 100%; */
    display: flex;
    height: 53rpx;
    flex-direction: row;
    /* position: relative; */
    border: 1rpx solid #959595;
    border-radius: 10rpx;
  }
  
