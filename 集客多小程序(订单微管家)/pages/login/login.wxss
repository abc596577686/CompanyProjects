  .goodsNumberWrap .handle .btn {
    width: 64rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    position: relative;
  }
  
  .goodsNumberWrap .handle .inputWrap {
    width: 78rpx;
    position: relative;
  }
  
  .goodsNumberWrap .handle .inputWrap::after,
  .goodsNumberWrap .handle .inputWrap::before {
    position: absolute;
    content: '';
    width: 1rpx;
    height: 100%;
    background: #ccc;
    transform: scaleX(0.5);
    top: 0;
  }
  
  .goodsNumberWrap .handle .inputWrap::after {
    left: 0;
  }
  
  .goodsNumberWrap .handle .inputWrap::before {
    right: 0;
  }
  
  .goodsNumberWrap .handle .inputWrap input {
    text-align: center;
    font-size: 28rpx;
    color: #333;
    border: none;
    outline: none;
  }
  
  .goodsNumberWrap .handle .btn.disabled {
    background: #f5f5f5;
  }
  
  .topLine {
    position: relative;
  }
  
  .topLine::after {
    position: absolute;
    content: '';
    left: 0;
    right: 0;
    top: 0;
    height: 1rpx;
    background: #f0f0f0;
    transform: scaleY(0.5);
  }
  
