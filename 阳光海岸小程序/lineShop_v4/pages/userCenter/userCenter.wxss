.projectWrap .item {
  padding: 32rpx 0;
  display: flex;
  flex-direction: row;
  position: relative;
  justify-content: space-between;
  font-size: 24rpx;
  color: #333;
}

.projectWrap .item::after {
  position: absolute;
  content: '';
  bottom: 0;
  left: 0;
  width: 100%;
  height: 1rpx;
  background: #ccc;
  transform: scaleY(0.5);
}

.projectWrap .item .left, .projectWrap .item .right {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.projectWrap .item .left {
  margin-left: 24rpx;
  font-size: 28rpx;
}

.projectWrap .item .left .imgWrap {
