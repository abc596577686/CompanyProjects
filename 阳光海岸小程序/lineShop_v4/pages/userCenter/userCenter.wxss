}

.textInfo .phone {
  font-size: 30rpx;
  color: #333;
  display: block;
  line-height: 40rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
}

.vipWrap {
