  align-items: center;
}

.avatar {
  width: 104rpx;
  height: 104rpx;
  overflow: hidden;
  border-radius: 100%;
  margin-right: 40rpx;
  border: 3rpx solid #fff;
  box-shadow: 5rpx 5rpx 10rpx 1rpx #a1a4b1;
